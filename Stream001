kapelusze = [Hats.Brown_Hat, Hats.Carrot_Hat, Hats.Gray_Hat, Hats.Green_Hat, Hats.Purple_Hat, Hats.Straw_Hat, Hats.Traffic_Cone]
k = 0
for i in range(2):
	for i in range(get_world_size()):
		till()
		move(North)
	move(East)
while True:
	for i in range(get_world_size()):
		harvest()
		if get_water() < 0.5:
			use_item(Items.Water)
		plant(Entities.Carrot)
		move(North)
	move(East)
	for i in range(get_world_size()):
		harvest()
		if get_water() < 0.5:
			use_item(Items.Water)
		move(North)
	move(East)
	for i in range(get_world_size()):
		harvest()
		if get_water() < 0.5:
			use_item(Items.Water)
		plant(Entities.Bush)
		move(North)
	move(East)
	
